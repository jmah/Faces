WRAPPER_EXTENSION = app
INSTALL_PATH = $(HOME)/Applications
INSTALL_MODE_FLAG = u+w,go-w,a+rX
PRECOMPS_INCLUDE_HEADERS_FROM_BUILT_PRODUCTS_DIR = NO
GCC_PRECOMPILE_PREFIX_HEADER = YES
GCC_DYNAMIC_NO_PIC = YES
